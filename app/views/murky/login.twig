        <section id="home" class="home-banner gray-bg">
            <div id="particles_js" class="particles-js-canvas-el"></div>
            <div class="container">
                <div class="row p-100px-t justify-content-center">
                    <div class="col-md-10 col-lg-8 text-center p-60px-tb sm-p-30px-tb">
                        <div class="home-text-center">
                            <h1 class="font-alt">Türkiye’nin En Kaliteli<br>Sosyal Medya Paneli</h1>
                            <div style="display:none;" class="alert alert-danger"> Sorunlar Düzeltilmiştir. </div>
                            <div class="app-btn-set">
                                <button type="button" class="m-btn m-btn-white" data-toggle="modal" data-target="#giris">Giris Yap</button>
                                <button type="button" class="m-btn m-btn-white" data-toggle="modal" data-target="#kayit">Kayıt Ol</button>
                                <p></p>


                            </div>
                            <p></p>
                        </div>
                    </div>
                </div>
            </div>
